{
    "runner.bootstrap": "vendor/autoload.php",
    "runner.path": "src/Benchmarks",
    "report.generators": {
        "consumation_of_time": {
            "extends": "default",
            "title": "The Consumation of Time",
            "description": "Benchmark how long it takes to consume time",
            "cols": [ "subject", "mode" ]
        }
    },
    "report.outputs": {
        "build-artifact": {
            "renderer": "html",
            "path": "var/report.html",
            "title": "Benchmarks"
        }
    }
}